{
  "name": "tfs-api",
  "version": "1.0.0",
  "description": "Integração com TFS Microsoft Server",
  "main": "./dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "compile": "tsc --sourcemap",
    "dev": "set NODE_ENV=development&& ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/server.ts",
    "dev-inspect": "ts-node-dev --inspect=0.0.0.0:7001 --exit-child --respawn src/server.ts",
    "devmon": "set NODE_ENV=development&& node --inspect=5858 -r ts-node/register/transpile-only ./src/server.ts",
    "devmon:watch": "nodemon",
    "start": "set NODE_ENV=production&& node ./dist/server.js",
    "prod": "npm run build && npm run start"
  },
  "keywords": [
    "api",
    "ts",
    "rest"
  ],
  "author": "Filipe Campos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filipercampos/tfs-api/issues"
  },
  "homepage": "https://github.com/filipercampos/tfs-api#readme",
  "repository": "git+https://github.com/filipercampos/tfs-api.git",
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "helmet": "^4.1.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
